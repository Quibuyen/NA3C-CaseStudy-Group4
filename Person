
public class Person {
    private String address;
    private String model;

    //Method 3: 
     public void getAdress(String address)
    {
        this.address = address;
    }
    
    public String showAddress()
    {
        return address;
    }
    
    //Method 4: 
    public void getModel(String model)
    {
        this.model = model;
    }
    
    public String showWork()
    {
        return model;
}
}
